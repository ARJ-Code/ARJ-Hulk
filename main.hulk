type Positions(start, last){
    start = start;
    last = last;
    current = start - 2;

    next(): Boolean => (self.current := self.current+1) <= self.last;
    current(): Number => self.current;
    reset(): Number => self.current := self.start;
}

let test = new Positions(1,5) in 
for (i in test){
    print(i);
}